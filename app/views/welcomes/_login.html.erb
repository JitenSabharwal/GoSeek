<%=form_for :user,url:"welcomes" do |f|%>
    <div class="form-group">
		<%=f.label :email %>
	</div>
    <div class="form-group">
		<%=f.email_field :email,class: "form-control input-lg"%>
	</div>
    <div class="form-group">
		<%=f.label :password %>
	</div>
    <div class="form-group">
		<%=f.password_field :password,class: "form-control input-lg"%>
	</div>
    <div class="form-group">
		<%=f.submit "login" ,class: "btn btn-primary"%>
	</div>
<% end %>